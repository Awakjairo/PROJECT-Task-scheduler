{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["warn", 2],
    "linebreak-style": "off",
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"],
    "no-unused-vars": "warn",
    "no-console": "warn"
  }
} 